{
    "version": 2,
    "builds": [
        {
            "src": "client/src/pages/**/*.tsx",
            "use": "@vercel/static"
        },
        {
            "src": "server/**/*.js",
            "use": "@vercel/node"
        }
        ],
        "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/$1"
        },
        {
            "src": "/(.*)",
            "dest": "client/src/pages/$1"
        }
    ]
}